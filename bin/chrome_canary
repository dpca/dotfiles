#!/usr/bin/env bash

CHROME_DIR="${HOME}/Chrome"
CHROME_BETA="${CHROME_DIR}/Google Chrome Canary.app"

if [ ! -d "$CHROME_BETA" ]; then
  echo "Chrome canary not installed at '${CHROME_BETA}'"
  exit 1
fi

"${CHROME_BETA}/Contents/MacOS/Google Chrome Canary" --user-data-dir="${CHROME_DIR}/chromecanaryprofile"
